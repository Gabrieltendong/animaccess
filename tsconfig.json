{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "baseUrl": "." ,
    "typeRoots": ["./src/types"],
    "paths": {
       "@constants/*": [
         "src/constants/*"
       ],
       "@interfaces/*": ["src/interfaces/*"],
       "@config/*": ["src/config/*"],
       "@assets/*": [
         "src/assets/*"
       ],
       "@components/*": [
         "src/components/*"
       ],
       "@screens/*": [
         "src/screens/*"
       ],
       "@themes/*": [
         "src/themes/*"
       ],
       "@utils/*": [
         "src/utils/*"
       ]
    }
  }
}
